{
  "name": "scrape",
  "runtime": "nodejs4.3",
  "description": "Serverless Lambda function for project: billboard-tv-serverless2",
  "customName": false,
  "customRole": false,
  "handler": "scrape/handler.handler",
  "timeout": 6,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": []
  },
  "endpoints": [],
  "events": [
    {
      "name": "every saturday",
      "type": "schedule",
      "config": {
        "schedule": "cron(0 0 ? * SAT *)"
      }
    }
  ],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}
